{
  "uid": "test-sli",
  "title": "TEST â€” SLI/SLO Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "graphTooltip": 0,
  "time": { "from": "now-1h", "to": "now" },
  "annotations": { "list": [] },
  "panels": [
    {
      "type": "timeseries",
      "title": "Request rate (RPS)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (method, path) (rate(http_requests_total[1m]))",
          "legendFormat": "{{method}} {{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": { "legend": { "showLegend": true, "displayMode": "list" } }
    },
    {
      "type": "timeseries",
      "title": "Error ratio (Availability SLI)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "100 * ( sum(rate(http_requests_total{status=~\"5..\"}[1m])) / sum(rate(http_requests_total[1m])) )",
          "legendFormat": "error %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "red", "value": 0.1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "legend": { "showLegend": false } }
    },
    {
      "type": "timeseries",
      "title": "Latency percentiles (/work)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.5,  sum by (le) (rate(http_request_duration_seconds_bucket{path=\"/work\"}[1m])))",
          "legendFormat": "p50"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.9,  sum by (le) (rate(http_request_duration_seconds_bucket{path=\"/work\"}[1m])))",
          "legendFormat": "p90"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_request_duration_seconds_bucket{path=\"/work\"}[1m])))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "red", "value": 0.3 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "legend": { "showLegend": true, "displayMode": "list" } }
    },
    {
      "type": "timeseries",
      "title": "In-flight requests",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (path) (http_inprogress_requests)",
          "legendFormat": "{{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": { "legend": { "showLegend": true, "displayMode": "list" } }
    }
  ]
}

